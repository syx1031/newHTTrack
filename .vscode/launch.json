    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug libtool",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/httrack",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            //"miDebuggerPath": "/usr/bin/libtool",
            //"miDebuggerArgs": "--mode=execute gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": false
                },
            ],
        }
    ]
}